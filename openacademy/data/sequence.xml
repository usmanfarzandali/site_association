<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for openacademy_student -->
        <record id="seq_openacademy_student" model="ir.sequence">
            <field name="name">Student Sequence</field>
            <field name="code">openacademy_student.sequence</field>
            <field name="prefix">OS</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_openacademy_teacher" model="ir.sequence">
            <field name="name">Teacher Sequence</field>
            <field name="code">openacademy_teacher.sequence</field>
            <field name="prefix">OT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>